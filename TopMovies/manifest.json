{
  "manifest_version": 2,

  "name": "Top Movies",
  "description": "Click movie icon to show list of top movies.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}